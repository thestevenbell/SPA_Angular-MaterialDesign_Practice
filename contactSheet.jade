md-bottom-sheet(class="md-list md-has-header")

    md-subheader
        | Contact
        span(class="name")
        {{ vm.user.name }}

    md-list
      md-list-item(ng-repeat="item in vm.items")
        md-button(ng-click="vm.contactUser(item)")
            md-icon(md-svg-icon="{{ item.icon_url }}")
            {{item.name}}


